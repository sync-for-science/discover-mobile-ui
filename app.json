{
  "expo": {
    "name": "Discovery Mobile",
    "slug": "discovery-mobile-ui",
    "owner": "sync-for-science",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "edu.s4s.discovery-mobile",
      "buildNumber": "0.1.0",
      "supportsTablet": true
    },
    "android": {
      "package": "edu.s4s.discovery-mobile",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
